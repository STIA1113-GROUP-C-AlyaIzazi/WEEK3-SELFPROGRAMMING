package selfProgramming;

public class exercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        double bookPrice = 31.89;
        double discount = 0.3*bookPrice;
        double newPrice = bookPrice-discount;
        
        System.out.printf ("The price of the book is RM%.2f" ,newPrice);
	}

}
