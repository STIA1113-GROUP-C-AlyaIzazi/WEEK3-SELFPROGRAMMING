package selfProgramming;

public class exercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        double ticketPrice = 31.89;
        double discount = 0.3*ticketPrice;
        double newPrice = ticketPrice-discount;
        
        System.out.printf ("The price of the ticket is RM%.2f" ,newPrice);
	}

}
